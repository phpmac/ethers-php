{
    "name": "phpmac/ethers-php",
    "description": "PHP SDK for Ethereum, inspired by ethers.js v6",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "ethereum",
        "ethers",
        "web3",
        "blockchain",
        "smart-contract",
        "evm"
    ],
    "autoload": {
        "psr-4": {
            "Ethers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Ethers\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "ext-bcmath": "*",
        "ext-gmp": "*",
        "guzzlehttp/guzzle": "^7.0",
        "kornrunner/keccak": "^1.1",
        "kornrunner/secp256k1": "^0.3.0",
        "simplito/elliptic-php": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "test:verbose": "phpunit --testdox --colors=always"
    },
    "config": {
        "sort-packages": true
    }
}